{
    "extends": "@parcel/config-default",
    "transformers": {
        "*.hbs": [
            "@anikin/parcel-transformer-handlebars-precompile"
        ],
        "*.{ts}": [
            "@parcel/transformer-typescript-tsc"
        ],
        "types:*.{ts, html, htm}": [
            "@parcel/transformer-typescript-types"
        ],
        "*.scss": ["@parcel/transformer-sass"]
    }
}
